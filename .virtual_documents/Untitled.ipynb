import pandas as pd
import os

# ğŸ“ ì—‘ì…€ íŒŒì¼ì´ ìˆëŠ” í´ë” ê²½ë¡œ
folder_path = "ìƒˆ í´ë”"  # ì‹¤ì œ ê²½ë¡œë¡œ ë°”ê¾¸ì„¸ìš”

# ğŸ“‚ ì—‘ì…€ íŒŒì¼ ëª©ë¡ ìˆ˜ì§‘
file_list = [f for f in os.listdir(folder_path) if f.endswith('.xlsx')]

# ğŸ§¹ íŒŒì¼ ë³‘í•©
merged_data = []

for file_name in file_list:
    file_path = os.path.join(folder_path, file_name)
    df = pd.read_excel(file_path, sheet_name=0)

    # 'ì •ì‚°ì¼ì'ê°€ ìˆëŠ” í–‰ ì°¾ê¸°
    start_idx = df[df.iloc[:, 0] == "ì •ì‚°ì¼ì"].index
    if len(start_idx) == 0:
        continue
    start_idx = start_idx[0]

    # ì»¬ëŸ¼ ì§€ì • ë° ë°ì´í„° ì¶”ì¶œ
    df.columns = df.iloc[start_idx]
    df = df.iloc[start_idx + 1:].reset_index(drop=True)

    # í•„ìš”í•œ ì—´ë§Œ ì„ íƒ
    df["íŒŒì¼ëª…"] = file_name
    df = df[["ì •ì‚°ì¼ì", "ì´ ë§¤ì¶œê¸ˆì•¡", "ì´ ì£¼ë¬¸ê±´ìˆ˜", "íŒŒì¼ëª…"]]

    merged_data.append(df)

# ğŸ“Š ì „ì²´ ë³‘í•©
final_df = pd.concat(merged_data, ignore_index=True)

# ğŸ’¾ CSV íŒŒì¼ë¡œ ì €ì¥
final_df.to_csv("df.csv", index=False, encoding='utf-8-sig')



